  <rdf:Description rdf:about="http://data.semanticweb.org/conference/eswc/2015/paper/research/141">
    <dc:subject>user involvement</dc:subject>
    <rdf:type rdf:resource="http://swrc.ontoware.org/ontology#InProceedings"/>
    <foaf:maker rdf:resource="http://data.semanticweb.org/person/johan-aberg"/>
    <dc:subject>large-scale ontology alignment</dc:subject>
    <dc:creator rdf:resource="http://data.semanticweb.org/person/johan-aberg"/>
    <swrc:month>May</swrc:month>
    <swc:isPartOf rdf:resource="http://data.semanticweb.org/conference/eswc/2015/proceedings"/>
