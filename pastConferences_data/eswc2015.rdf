  <rdf:Description rdf:about="http://data.semanticweb.org/conference/eswc/2015/tutorial/01293a05-d307-3ed8-904b-bbc7611e4282_5">
    <icaltzd:lastModified rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2015-05-11T20:16:14Z</icaltzd:lastModified>
    <icaltzd:dtstamp rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2015-05-11T20:16:14Z</icaltzd:dtstamp>
