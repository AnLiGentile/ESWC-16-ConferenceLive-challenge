    <foaf:maker rdf:resource="http://data.semanticweb.org/person/gerard-de-melo"/>
    <rdfs:label>FrameBase: Representing N-ary Relations using Semantic Frames</rdfs:label>
    <dc:title>FrameBase: Representing N-ary Relations using Semantic Frames</dc:title>
