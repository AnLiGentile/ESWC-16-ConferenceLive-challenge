  <rdf:Description rdf:about="http://data.semanticweb.org/conference/eswc/2015/paper/poster/42">
    <eswc-owl:hashtag rdf:datatype="http://www.w3.org/2001/XMLSchema#string">#eswc2015PD</eswc-owl:hashtag>
    <dc:creator rdf:resource="http://data.semanticweb.org/person/andy-moniz"/>
