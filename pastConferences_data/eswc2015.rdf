  <rdf:Description rdf:about="http://data.semanticweb.org/conference/eswc/2015/paper/research/171">
    <foaf:maker rdf:resource="http://data.semanticweb.org/person/haridimos-kondylakis"/>
    <swc:isPartOf rdf:resource="http://data.semanticweb.org/conference/eswc/2015/proceedings"/>
