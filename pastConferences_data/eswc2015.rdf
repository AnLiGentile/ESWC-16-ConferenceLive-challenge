    <dc:title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Rubya: a tool for generating rules for incremental maintenance of RDF views</dc:title>
    <rdf:type rdf:resource="http://swrc.ontoware.org/ontology#InProceedings"/>
