    <dc:subject>SKOS</dc:subject>
    <dc:subject>Knowledge Organization Systems</dc:subject>
    <rdfs:label>VocBench: a Web Application for Collaborative Development of Multilingual Thesauri</rdfs:label>
    <eswc-owl:isPresented rdf:resource="http://data.semanticweb.org/conference/eswc/2015/talk/0f850b3c-74da-3bbf-a0a3-c2b85931d6a6_27"/>
    <dc:subject>Workflow Management</dc:subject>
    <dc:subject>Collaborative Editing</dc:subject>
